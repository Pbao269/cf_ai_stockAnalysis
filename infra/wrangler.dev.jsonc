{
  "extends": "./wrangler.common.jsonc",
  "name": "stock-report-dev",
  "vars": {
    "ENVIRONMENT": "development"
  },
  "service_bindings": [
    {
      "name": "intent",
      "service": "intent-dev"
    },
    {
      "name": "screener", 
      "service": "screener-dev"
    },
    {
      "name": "fundamentals-dcf",
      "service": "fundamentals-dcf-dev"
    },
    {
      "name": "technicals",
      "service": "technicals-dev"
    },
    {
      "name": "catalyst-sentiment",
      "service": "catalyst-sentiment-dev"
    },
    {
      "name": "entry-dca",
      "service": "entry-dca-dev"
    },
    {
      "name": "user-data",
      "service": "user-data-dev"
    },
    {
      "name": "notion-export",
      "service": "notion-export-dev"
    },
    {
      "name": "etl-workflows",
      "service": "etl-workflows-dev"
    }
  ],
  "kv_namespaces": [
    {
      "binding": "FUNDAMENTALS_SNAP",
      "id": "your-fundamentals-snap-namespace-id-dev"
    },
    {
      "binding": "LATEST_QUOTES", 
      "id": "your-latest-quotes-namespace-id-dev"
    },
    {
      "binding": "SCREENER_INDEX",
      "id": "your-screener-index-namespace-id-dev"
    }
  ],
  "r2_buckets": [
    {
      "binding": "OHLCV",
      "bucket_name": "ohlcv-dev"
    }
  ],
  "d1_databases": [
    {
      "binding": "USER_DB",
      "database_id": "your-user-db-id-dev",
      "database_name": "user-db-dev"
    }
  ],
  "ai": {
    "binding": "AI"
  }
}
